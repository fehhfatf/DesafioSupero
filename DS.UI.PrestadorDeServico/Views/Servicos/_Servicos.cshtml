@model IEnumerable<DS.Domain.Entities.Servico>

@*<div id="divListaServicos">*@
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cliente.Nome)
            </th>
            <th>
                <p>Bairro</p>
            </th>
            <th>
                <p>Cidade</p>
            </th>
            <th>
                <p>Estado</p>
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoDeServico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorDoAtendimento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataDeAtendimento)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Cliente.Nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cliente.Bairro)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cliente.Cidade)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cliente.Estado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TipoDeServico)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ValorDoAtendimento)
                </td>                
                <td>
                    @Html.DisplayFor(modelItem => item.DataDeAtendimento)
                </td>
            </tr>
        }
    </table>
@*</div>*@